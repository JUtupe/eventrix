{
  "name": "eventrix",
  "version": "1.12.1",
  "description": "store based on events",
  "main": "index.js",
  "repository": "https://github.com/mprzodala/eventrix",
  "homepage": "https://github.com/mprzodala/eventrix",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "webpack --config webpack-prod.config.js --stats-error-details",
    "coverage": "jest --coverage",
    "coverage:report": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint -c .eslintrc.json **/*.js? --ignore-path .gitignore",
    "lint:fix": "eslint --fix -c .eslintrc.json **/*.js? --ignore-path .gitignore"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "keywords": [
    "store"
  ],
  "author": "Mariusz Przoda≈Ça",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^17.0.4",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0 || ^17.0.0",
    "react-dom": "^16.0.0 || ^17.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.6.3",
    "@babel/traverse": "^7.7.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "coveralls": "^3.1.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "7.13.0",
    "jest": "^24.9.0",
    "loader-utils": "^2.0.0",
    "pre-commit": "^1.2.2",
    "react-proptypes": "^1.0.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0"
  },
  "typings": "./types.ts",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "collectCoverage": true
  }
}
